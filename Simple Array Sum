import java.util.*;
public class Solution
{
    public static void main(String args[])
    {
        Scanner in=new Scanner(System.in);
        int n,sum=0,d;
        n=in.nextInt();
        for(int i=0;i<n;i++)
        {
            d=in.nextInt();
            sum=sum+d;
        }
        System.out.println(sum);
    }
}
