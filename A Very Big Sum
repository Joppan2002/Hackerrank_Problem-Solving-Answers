import java.util.*;
import java.math.*;
public class Solutions
{
    public static void main(String args[])
    {
        Scanner in=new Scanner(System.in);
        int n,i;
        BigInteger a;
        BigInteger sum=new BigInteger("0");
        String str;
        n=in.nextInt();
        for(i=0;i<n;i++)
        {
            str=in.next();
            a=new BigInteger(str);
            sum=sum.add(a);
        }
        System.out.println(sum);
    }
}
