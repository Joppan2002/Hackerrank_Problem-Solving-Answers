import java.util.*;
public class Solution
{
    public static void main(String args[])
    {
        Scanner in=new Scanner(System.in);
        int n,i,j;
        n=in.nextInt();
        for(i=1;i<=n;i++)
        {
            for(j=0;j<n-i;j++)
            {
                System.out.print(" ");
            }
            for(j=0;j<i;j++)
            {
                System.out.print("#");
            }
            System.out.println();
        }
    }
}
